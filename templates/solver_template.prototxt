net: "$TRAIN_PROTOTXT"

# Testing
test_iter: $TEST_ITER$
test_interval: 20

# Schedule
base_lr: $BASE_LR$
lr_policy: "step"
stepsize: $STEPSIZE$
max_iter: $MAX_ITER$
gamma: 0.1

# Solver
momentum: 0.9
weight_decay: 0.0005

# Visualization
display: 5
snapshot: 0
snapshot_prefix: "$SNAPSHOT_PREFIX$"
solver_mode: GPU
